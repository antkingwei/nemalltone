<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.rolmex.android.nemalltone.widget.ObservableScrollView 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/pagetwo_scrollview"
        >
        <FrameLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"            
            >
            <com.rolmex.android.nemalltone.widget.MyGridView 
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/pagetwo_gridview"
                ></com.rolmex.android.nemalltone.widget.MyGridView>
        </FrameLayout>
    </com.rolmex.android.nemalltone.widget.ObservableScrollView>

</RelativeLayout>